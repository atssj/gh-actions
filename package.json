{
  "name": "gh-actions",
  "version": "0.1.0",
  "description": "A collection of Github actions.",
  "scripts": {
    "format": "bunx prettier . --write",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish && ./scripts/release.sh",
    "commitlint": "bunx --bun commitlint --edit",
    "prepare": "husky"
  },
  "keywords": [
    "Github actions"
  ],
  "author": "S Saha",
  "license": "UNLICENSED",
  "packageManager": "bun@1.2.4",
  "engines": {
    "node": ">=20.10.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "husky": "^9.1.7",
    "prettier": "^3.2.5"
  },
  "workspaces": [
    "packages/*",
    "actions/*"
  ]
}
